# Environment variables for AI Content Generation Project

# OpenAI API configuration
OPENAI_API_KEY=your_openai_api_key_here

# Database configuration (if applicable)
# DB_HOST=localhost
# DB_USER=your_db_user
# DB_PASS=your_db_password
# DB_NAME=your_db_name

# Application configuration
APP_SECRET_KEY=your_secret_key_here
APP_DEBUG=True

# Content generation settings
DEFAULT_LANGUAGE=en
MAX_TOKENS=256

# Email settings for notifications or email marketing (if applicable)
# EMAIL_HOST=smtp.your-email-provider.com
# EMAIL_PORT=587
# EMAIL_USE_TLS=True
# EMAIL_HOST_USER=your_email_address
# EMAIL_HOST_PASSWORD=your_email_password

# Other API keys (if any other APIs are integrated)
# OTHER_API_KEY=your_other_api_key_here

# Logging configuration
LOG_LEVEL=INFO

# AI Model fine-tuning settings (if applicable)
# MODEL_NAME=your_pretrained_model_name
# FINE_TUNING_DATASET_PATH=path_to_your_dataset

# Content optimization settings
# SEO_KEYWORDS=keyword1,keyword2,keyword3

# Multilingual support (if applicable)
# SUPPORTED_LANGUAGES=en,es,fr,de

# Content personalization settings (if applicable)
# USER_BEHAVIOR_ANALYTICS_API_KEY=your_analytics_api_key

# A/B Testing settings (if applicable)
# AB_TESTING_API_KEY=your_ab_testing_api_key

